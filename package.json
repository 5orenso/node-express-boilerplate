{
  "name": "node-express-boilerplate",
  "description": "A nice boilerplate with a template engine, unit tests, integration tests, code coverage, continuous integration, code analysis and a nice setup flow.",
  "version": "3.0.0",
  "homepage": "https://github.com/5orenso/node-express-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/5orenso/node-express-boilerplate"
  },
  "author": {
    "name": "Øistein Sørensen",
    "email": "sorenso@gmail.com"
  },
  "main": "app/server.js",
  "engines": {
    "node": ">=6.11"
  },
  "scripts": {
    "test": "grunt",
    "test-jest": "jest"
  },
  "devDependencies": {
    "buster": "^0.7.18",
    "buster-istanbul": "^0.1.15",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "grunt": "^1.0.3",
    "grunt-buster": "^0.4.2",
    "grunt-contrib-nodeunit": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-coveralls": "^2.0.0",
    "grunt-eslint": "^21.0.0",
    "grunt-jsdoc": "^2.2.1",
    "grunt-nodemon": "^0.4.2",
    "grunt-retire": "^1.0.7",
    "grunt-shell": "~2.1.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^23.4.2",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-habitat": "^3.1.0",
    "preact-router": "^2.5.7",
    "request": "~2.87.0",
    "time-grunt": "^1.4.0"
  },
  "keywords": [],
  "dependencies": {
    "body-parser": "^1.18.3",
    "commander": "~2.17.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "express": "~4.16.3",
    "express-useragent": "^1.0.12",
    "highlight.js": "^9.12.0",
    "is-my-json-valid": "~2.17.2",
    "marked": "^0.4.0",
    "maxmind": "^2.7.0",
    "morgan": "~1.9.0",
    "node-dogstatsd": "0.0.7",
    "swig": "~1.4.2",
    "underscore": "~1.9.1",
    "underscore-contrib": "~0.3.0",
    "when": "~3.7.8",
    "winston": "~3.0.0",
    "winston-syslog": "~2.0.0"
  },
  "jest": {
    "verbose": true,
    "rootDir": "preact/",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
